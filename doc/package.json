{
    "name": "@webpm-client/doc",
    "description": "Documentation of @w3nest/webpm-client",
    "version": "0.1.3-wip",
    "author": "greinisch@youwol.com",
    "homepage": "https://github.com/webpm-client/doc#README.md",
    "main": "dist/index.html",
    "types": "src/index.ts",
    "scripts": {
        "clean": "del-cli dist",
        "setup": "python .w3nest/setup.py",
        "build": "yarn build:dev",
        "pre-build": "yarn clean",
        "build:dev": "yarn pre-build && webpack --mode development",
        "build:prod": "yarn pre-build && webpack --mode production",
        "lint-check": "yarn lint-prettier-check && yarn lint-eslint-check",
        "lint-prettier-check": "prettier --check ./src",
        "lint-prettier-write": "prettier --write ./src",
        "lint-eslint-check": "eslint ./src",
        "test": "jest -i",
        "test-coverage": "jest -i --collect-coverage",
        "doc": "npx tsx .w3nest/doc.ts",
        "publish-npm-public": "yarn build:prod && yarn test-coverage && yarn publish --access public",
        "start": "webpack serve"
    },
    "dependencies": {
        "@w3nest/webpm-client": "^0.1.3",
        "mkdocs-ts": "^0.3.0",
        "rxjs": "^7.5.6"
    },
    "devDependencies": {
        "@types/jest": "^29.5.4",
        "@types/node": "^22.9.0",
        "@types/webpack": "^5.28.5",
        "css-loader": "^6.8.1",
        "del-cli": "^5.1.0",
        "eslint": "9.14.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-jest": "^28.9.0",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.5.3",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-junit": "^16.0.0",
        "mini-css-extract-plugin": "^2.9.2",
        "prettier": "^3.2.5",
        "prettier-plugin-properties": "^0.3.0",
        "source-map-loader": "5.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "9.5.1",
        "ts-node": "10.9.2",
        "tsx": "4.19.2",
        "typedoc": "^0.27.6",
        "typescript": "5.6.3",
        "typescript-eslint": "8.14.0",
        "webpack": "^5.96.1",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "^5.1.0"
    },
    "webpm": {
        "dependencies": {
            "@w3nest/webpm-client": "^0.1.3",
            "mkdocs-ts": "^0.3.0",
            "rxjs": "^7.5.6"
        },
        "aliases": []
    }
}
