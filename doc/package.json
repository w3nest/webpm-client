{
    "name": "@webpm-client/doc",
    "description": "Doc. app for WebPM client usage",
    "version": "0.1.4",
    "author": "reinisch.gui@gmail.com",
    "homepage": "https://github.com/webpm-client/doc#README.md",
    "main": "./dist/@webpm-client/doc.js",
    "types": "src/index.ts",
    "scripts": {
        "clean": "del-cli dist",
        "setup": "python .w3nest/setup.py",
        "build": "yarn build:prod",
        "pre-build": "yarn clean",
        "build:dev": "yarn pre-build && webpack --mode development",
        "build:prod": "yarn pre-build && webpack --mode production",
        "lint-check": "yarn lint-prettier-check && yarn lint-eslint-check",
        "lint-prettier-check": "prettier --check ./src",
        "lint-prettier-write": "prettier --write ./src",
        "lint-eslint-check": "eslint ./src",
        "test": "jest -i",
        "test-coverage": "jest -i --collect-coverage",
        "doc": "npx tsx .w3nest/doc.ts",
        "publish-npm-public": "yarn build:prod && yarn test-coverage && yarn publish --access public",
        "start": "webpack serve --mode development"
    },
    "exports": {
        ".": {
            "default": "./dist/@webpm-client/doc.js",
            "types": "./src/app/main.ts",
            "webpm": {
                "dependencies": [
                    "mkdocs-ts",
                    "@w3nest/webpm-client",
                    "rxjs",
                    "@w3nest/http-clients",
                    "rx-vdom",
                    "@w3nest/ui-tk/Badges"
                ],
                "aliases": []
            }
        }
    },
    "dependencies": {
        "@w3nest/http-clients": "^0.1.5",
        "@w3nest/ui-tk": "^0.1.0",
        "@w3nest/webpm-client": "^0.1.5",
        "mkdocs-ts": "^0.3.2",
        "rx-vdom": "^0.1.3",
        "rxjs": "^7.5.6"
    },
    "devDependencies": {
        "@types/jest": "^29.5.4",
        "@types/node": "^22.13.4",
        "@types/webpack": "^5.28.5",
        "css-loader": "^6.8.1",
        "del-cli": "^5.1.0",
        "eslint": "9.14.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-jest": "^28.9.0",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.6.3",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-junit": "^16.0.0",
        "mini-css-extract-plugin": "^2.9.2",
        "prettier": "^3.2.5",
        "prettier-plugin-properties": "^0.3.0",
        "source-map-loader": "5.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "9.5.2",
        "ts-node": "10.9.2",
        "tsx": "^4.19.2",
        "typedoc": "^0.27.7",
        "typescript": "5.7.3",
        "typescript-eslint": "8.14.0",
        "webpack": "^5.98.0",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "6.0.1",
        "webpack-dev-server": "^5.2.0"
    },
    "webpm": {
        "kind": "webapp",
        "dependencies": {
            "@w3nest/http-clients": "^0.1.5",
            "@w3nest/ui-tk": "^0.1.0",
            "@w3nest/webpm-client": "^0.1.5",
            "mkdocs-ts": "^0.3.2",
            "rx-vdom": "^0.1.3",
            "rxjs": "^7.5.6"
        },
        "html": "dist/index.html"
    },
    "webpack": {
        "root": "src",
        "apiVersion": "01",
        "main": "app/main.ts",
        "additionalEntries": {},
        "externals": {
            "mkdocs-ts": "window['mkdocs-ts_APIv03']",
            "@w3nest/webpm-client": "window['@w3nest/webpm-client_APIv01']",
            "rxjs": "window['rxjs_APIv7']",
            "@w3nest/http-clients": "window['@w3nest/http-clients_APIv01']",
            "rx-vdom": "window['rx-vdom_APIv01']",
            "@w3nest/ui-tk/Badges": "window['@w3nest/ui-tk_APIv01']['Badges']"
        }
    }
}
