{
    "name": "@w3nest/webpm-client",
    "description": "Library for dynamic npm's libraries installation from W3 Nest ecosystem.",
    "version": "0.1.16",
    "author": "reinisch.gui@gmail.com",
    "license": "MIT",
    "homepage": "https://github.com/w3nest/webpm-client#README.md",
    "main": "./dist/@w3nest/webpm-client.js",
    "files": [
        "dist",
        "assets"
    ],
    "scripts": {
        "clean": "del-cli dist",
        "setup": "python .w3nest/setup.py",
        "build": "yarn build:prod",
        "pre-build": "yarn clean",
        "post-build": "del-cli dist/src/tests",
        "build:dev": "yarn pre-build && webpack --mode development && yarn post-build",
        "build:prod": "yarn pre-build && webpack --mode production && yarn post-build",
        "lint-check": "yarn lint-prettier-check && yarn lint-eslint-check",
        "lint-prettier-check": "prettier --check ./src",
        "lint-prettier-write": "prettier --write ./src",
        "lint-eslint-check": "eslint ./src",
        "test": "jest -i",
        "test-coverage": "jest -i --collect-coverage",
        "doc": "typedoc",
        "publish-npm-public": "yarn build:prod && yarn test-coverage && yarn publish --access public"
    },
    "exports": {
        ".": {
            "default": "./dist/@w3nest/webpm-client.js",
            "types": "./dist/src/index.d.ts",
            "webpm": {
                "dependencies": [],
                "aliases": [
                    "webpm"
                ]
            }
        },
        "./workersPool": {
            "default": "./dist/workersPool.js",
            "types": "./dist/src/lib/workers-pool/index.d.ts",
            "webpm": {
                "dependencies": [
                    "rxjs"
                ],
                "aliases": []
            }
        },
        "./testUtils": {
            "default": "./dist/testUtils.js",
            "types": "./dist/src/lib/test-utils/index.d.ts",
            "webpm": {
                "dependencies": [],
                "aliases": []
            }
        },
        "./views": {
            "default": "./dist/views.js",
            "types": "./dist/src/lib/views/index.d.ts",
            "webpm": {
                "dependencies": [
                    "rxjs",
                    "rx-vdom"
                ],
                "aliases": []
            }
        }
    },
    "dependencies": {
        "@w3nest/http-clients": "^0.1.5",
        "rx-vdom": "^0.1.3",
        "rxjs": "^7.5.6",
        "semver": "^7.3.4"
    },
    "devDependencies": {
        "@jest/test-sequencer": "^29.5.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.3.0",
        "@types/webpack": "^5.28.5",
        "@w3nest/http-clients": "^0.1.5",
        "brotli": "^1.3.2",
        "del-cli": "^6.0.0",
        "eslint": "9.34.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-jest": "^29.0.1",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^30.1.2",
        "jest-environment-jsdom": "^30.1.2",
        "jest-junit": "^16.0.0",
        "prettier": "^3.6.2",
        "prettier-plugin-properties": "^0.3.0",
        "ts-jest": "^29.4.1",
        "ts-loader": "9.5.4",
        "ts-node": "10.9.2",
        "tsx": "^4.20.5",
        "typedoc": "^0.28.12",
        "typescript": "5.9.2",
        "typescript-eslint": "8.41.0",
        "util": "^0.12.5",
        "webpack": "^5.101.3",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "6.0.1"
    },
    "webpm": {
        "kind": "esm",
        "dependencies": {
            "@w3nest/http-clients": "^0.1.5",
            "rx-vdom": "^0.1.3",
            "rxjs": "^7.5.6"
        }
    },
    "webpack": {
        "root": "src",
        "apiVersion": "01",
        "main": "./index.ts",
        "additionalEntries": {
            "workersPool": "./lib/workers-pool/index.ts",
            "testUtils": "./lib/test-utils/index.ts",
            "views": "./lib/views/index.ts"
        },
        "externals": {
            "@w3nest/http-clients": [
                "@w3nest/http-clients_APIv01"
            ],
            "rx-vdom": [
                "rx-vdom_APIv01"
            ],
            "rxjs": [
                "rxjs_APIv7"
            ]
        }
    }
}
